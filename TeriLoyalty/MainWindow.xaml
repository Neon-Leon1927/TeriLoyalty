<Window x:Class="TeriLoyalty.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeriLoyalty"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="173*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>

        <Button Content="TestAdminWindow" Name="BtnTestAdminWindow" Click="BtnTestAdminWindow_Click"  Margin="10,10,577,10" Panel.ZIndex="1"/>
         
        <Grid Background="SeaGreen"></Grid>
        <Grid Grid.Row="2" Background="SeaGreen"></Grid>
        <TextBlock Text="TeriShop" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Width="114" ></TextBlock>
        <Frame NavigationUIVisibility="Hidden" Grid.Row="1" x:Name="MainFrame"></Frame>
        <Button Content="Back" Name="BtnBack" Background="LightSeaGreen" Margin="684,10,10,10" Click="BtnBack_Click" Panel.ZIndex="1"/>
    </Grid>
</Window>
