<Window x:Class="TeriLoyalty.EditLoyalAppWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeriLoyalty"
        mc:Ignorable="d"
        Title="EditLoyalAppWindow" Height="450" Width="800" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="173*"/>
            <RowDefinition Height="14*"/>
        </Grid.RowDefinitions>
        <Grid Background="SeaGreen"></Grid>
        <Grid Grid.Row="2" Background="SeaGreen"></Grid>
        <TextBlock Text="TeriShop" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Width="114" ></TextBlock>
        <TextBlock Text="Admin" FontSize="10" HorizontalAlignment="Left" VerticalAlignment="Top" Height="20" Width="58" ></TextBlock>
        <Frame Grid.Row="1" Name="EditLoyalAppFrame"></Frame>
        <Button Content="Back" Name="BtnBack" Background="LightSeaGreen" Margin="654,10,10,10" Click="BtnBack_Click"/>
    </Grid>
</Window>
